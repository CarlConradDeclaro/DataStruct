import java.util.*;
public class Main
{
   
	public static void main(String[] args) {
     LinkedList<Integer> list  = new LinkedList();
     
     int arr[] ={5,4,30,2,0};
     
     for(int i=0; i<arr.length;i++){
         list.add(arr[i]);
     }
     
     Collections.sort(list);
     list.add(4,350);
     System.out.println(list );
     list.remove(4);
     System.out.println(list);
     list.set(4,350);
     System.out.println(list);
	}
}
