 
class Main {
 
    public static void main(String[] args)
    {
      
      int[] arr = {4,6,2,1,9,63,-134,566,1000};
 
      // Find the largert value in a list;
      int temp = arr[0];
      for(int i=0; i<arr.length;i++){
         if(temp < arr[i]){
             temp = arr[i];
         }
      }
      
      System.out.println(temp);
      
    }
}
